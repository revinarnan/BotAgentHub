@model BotAgentHubApp.Models.ChatHistoryViewModels

@{
	ViewBag.Title = "Document View";
}

<h2>Riwayat Percakapan</h2>

<div>
	<dl class="dl-horizontal">
		<dt>
			Conversation ID
		</dt>
		<dd>
			@Html.DisplayFor(model => model.Transcript.ConversationId)
		</dd>

		<dt>
			Date
		</dt>
		<dd>
			@Html.DisplayFor(model => model.Transcript.Date)
		</dd>
			
		<dt>
			Message
		</dt>
		<dd>
			@foreach (var item in Model.Message.MessageList)
			{
            <div>@Html.DisplayFor(model => item)</div>
			}
		</dd>
	</dl>
</div>

<p>
	@Html.ActionLink("Back", "Index")
</p>